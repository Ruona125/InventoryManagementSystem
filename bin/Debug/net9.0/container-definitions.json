[
  {
    "name": "hostname",
    "image": "964832344816.dkr.ecr.us-east-1.amazonaws.com/hostname:latest",
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 8080
      }
    ],
    "essential": true,
    "environment": [
      {
        "name": "ASPNETCORE_ENVIRONMENT",
        "value": "Production"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/inventory-management-system-task-definition",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]
